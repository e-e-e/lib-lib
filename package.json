    "lint": "eslint ./src/*.ts ./test/*.ts",
    "lint:fix": "eslint ./src/*.ts ./test/*.ts --fix"
  },
  "author": "e-e-e",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/e-e-e/library-lib.git"
  },
  "keywords": [
    "isbn"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-utils": "^1.4.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.9.7"
  }
