{
  "name": "library-lib",
  "version": "0.0.1",
  "description": "A collection of helpful functions for parsing and manipulating library related data.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc -d",
    "build:docs": "typedoc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
